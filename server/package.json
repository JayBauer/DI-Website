{
  "name": "diagnostic-illumination-database",
  "description": "Database for Diagnostic Illumination Website",
  "author": "Jay Bauer (Elite Digital)",
  "version": "1.0.0",
  "main": "src/index.js",
  "keywords": [
    "diagnostic",
    "illumination",
    "diagnostic",
    "illumination",
    "mri"
  ],
  "license": "ISC",
  "scripts": {
    "start": "nodemon -e js,graphql -x node -r dotenv/config src/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect -r dotenv/config src/index.js",
    "playground": "graphql playground --server-only",
    "dev": "npm-run-all --parallel start playground"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "graphql-yoga": "1.6.1",
    "jsonwebtoken": "8.2.0",
    "prisma-binding": "1.5.16",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "dotenv": "5.0.1",
    "graphql-cli": "2.15.8",
    "mkdirp": "^0.5.1",
    "nodemon": "1.17.2",
    "npm-run-all": "4.1.2",
    "prisma": "1.6.0",
    "stripe": "^5.8.0"
  }
}
